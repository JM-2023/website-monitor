{
  "name": "website-monitor",
  "version": "0.0.5",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "npm run build && node .out/index.js",
    "dev": "node scripts/dev.mjs --mode=launch",
    "dev:attach": "node scripts/dev.mjs --mode=attach --browser-url=http://127.0.0.1:9222",
    "clean": "node scripts/clean.mjs",
    "test": "npm run build && node --test helpers.test.mjs tests/*.test.mjs"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/js-levenshtein": "^1.1.0",
    "@types/puppeteer": "^5.4.4",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "date-fns": "^2.23.0",
    "js-levenshtein": "^1.1.6",
    "puppeteer-core": "^10.2.0",
    "source-map-support": "^0.5.19"
  }
}
